package org.shark.async;

import java.util.List;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@RequestMapping("/a")
@Controller
public class AsyncControllerA {
  
  List<BoardDTO> boards = new ArrayList<BoardDTO>();
  
  public AsyncControllerA() {
    boards.add(new BoardDTO("제목1", 20));
    boards.add(new BoardDTO("제목2", 10));
    boards.add(new BoardDTO("제목3", 20));
  }
  
  @RequestMapping(value = {"/list","/list.json"})
  public void methodA() {
    
  }
  
  @RequestMapping(value = "/list.xml")
  public void methodB() {
    
  }
  
  
  
}
